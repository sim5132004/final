<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="party.people.repository.search.SearchInputMapper">



    <!-- 회원 가입 -->
    <insert id="save" useGeneratedKeys="true" keyProperty="sequenceId">
        <selectKey keyProperty="sequenceId" resultType="java.lang.Long" order="BEFORE">
            SELECT searchinput_seq.NEXTVAL FROM DUAL
        </selectKey>
        INSERT INTO searchinput (sequenceid, category, keyword, address)
        VALUES (#{sequenceId}, #{category}, #{keyword}, #{address})
    </insert>

    <select id = "loadAll" resultType="searchResult">
        select *
        from searchresult
    </select>



</mapper>
